package com.capgmini.helloworld;

public class MyIf {
	
	public static void main(String args[])
	{
		int a=-10;
		
		//Business Condition -> only positive number should print 
		
		// >  compare two value and return true or false
		//logical condition
		//true or false
		System.out.println(a>0);
		
		/*
		 * if() {
		 * 
		 * }
		 */
		
	
		System.out.println(a);
		
	}

}
