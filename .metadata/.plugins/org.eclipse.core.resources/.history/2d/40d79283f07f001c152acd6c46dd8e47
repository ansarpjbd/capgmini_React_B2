package com.cpagi.day2;

class Student
{

	 @Override
	public String toString() {
		return "Student [rollno=" + rollno + ", name=" + name + ", schoolName=" + schoolName + "]";
	}
	public Student(int rollno, String name, String schoolName) {
		super();
		this.rollno = rollno;
		this.name = name;
		this.schoolName = schoolName;
	}
	private int rollno;
	 private String name;
	 private static String schoolName;
	 
}


public class MyStatic {
	
	public static void main(String args[])
	{
		Student s1=new Student(1001, "Ansar","Abc School");
		Student s3=new Student(1002, "Priya","Abc School");
		Student s2=new Student(1003, "Mehesh","Abc School");
	}

}
