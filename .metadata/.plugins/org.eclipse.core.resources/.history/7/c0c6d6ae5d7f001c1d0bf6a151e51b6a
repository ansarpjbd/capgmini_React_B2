	{
		
		//for loop 
		//start value
		//condition
		//increment / decrement 
		
		//Business Condition -need to print hello world 10 times
		
		
		//   //until reached the number 10;
		
		//
		//i=i-1;
		
		//shortend :
		//i++;
		//i--
		
		for(int i=0;i>10;i=i+1;)
		{
			System.out.println("Hello World");
		}
		
		

	}

}
